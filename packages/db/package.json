{
  "name": "@acme/db",
  "private": true,
  "version": "0.1.0",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./schema": "./src/schema.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "format": "prettier --check \"**/*.{mjs,ts,md,json}\"",
    "format:fix": "prettier --write \"**/*.{mjs,ts,md,json}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "push": "drizzle-kit push",
    "studio": "pnpm with-env drizzle-kit studio",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --",
    "db:migrate": "tsx src/migrations/index.ts"
  },
  "dependencies": {
    "@vercel/postgres": "^0.10.0",
    "drizzle-orm": "^0.38.4",
    "drizzle-zod": "^0.6.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@acme/eslint-config": "^0.1.0",
    "@acme/typescript-config": "^0.1.0",
    "dotenv-cli": "^7.3.0",
    "drizzle-kit": "^0.30.2",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "tsx": "^4.1.1"
  },
  "prettier": "@acme/prettier-config"
}
